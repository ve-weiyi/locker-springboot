<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ve.locker.dao.PrivacyFolderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ve.locker.entity.PrivacyFolder">
        <id column="id" property="id" />
        <result column="folder_name" property="folderName" />
        <result column="folder_cover" property="folderCover" />
        <result column="folder_desc" property="folderDesc" />
        <result column="ownerId" property="ownerId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, folder_name, folder_cover, folder_desc, owner_id
    </sql>

</mapper>
